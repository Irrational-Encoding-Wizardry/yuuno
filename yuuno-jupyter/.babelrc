{
    "env": {
        "production": {
            "presets": [
                ["env", {
                  "modules": false,
                  "targets": {
                    "browsers": ["> 1%", "last 2 versions", "not ie <= 11"]
                  }
                }],
                "stage-2"
              ],
              "plugins": ["transform-vue-jsx", "transform-runtime", "transform-decorators-legacy"]
        },
        "development": {
            "presets": [
                "stage-2"
              ],
              "plugins": ["transform-vue-jsx", "transform-decorators-legacy", "transform-object-rest-spread"]
        }
    }
}